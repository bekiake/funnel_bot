# Telegram Bot Token - получите у @BotFather
TOKEN=your_bot_token_here

# Database URL для SQLite (по умолчанию)
DATABASE_URL=sqlite+aiosqlite:///my_base.db

# Для PostgreSQL (рекомендуется для продакшена):
# DATABASE_URL=postgresql+asyncpg://username:password@localhost/dbname

# Логирование
LOG_LEVEL=INFO

# Администраторы (можно добавить несколько через запятую)
ADMIN_IDS=1210278389

# Настройки платежей
PAYMENT_CARD_NUMBER=8600 1234 5678 9012
PAYMENT_CARD_OWNER=EXAMPLE NAME

# Настройки каналов
DEFAULT_CHANNEL_ID=-1001234567890
